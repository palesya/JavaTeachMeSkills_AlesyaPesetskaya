<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>car</servlet-name>
    <servlet-class>com.tms.servlet.CarServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet>
    <servlet-name>cookies</servlet-name>
    <servlet-class>com.tms.servlet.CookieServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>car</servlet-name>
    <url-pattern>/car</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>cookies</servlet-name>
    <url-pattern>/car/cookies</url-pattern>
  </servlet-mapping>

</web-app>
